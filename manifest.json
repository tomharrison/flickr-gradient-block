{
	"name": "Flickr Gradient Block",
	"short_name": "nogradients",
	"version": "0.0.5",
	"description": "Removes gradient overlays from images in your Flickr contacts feed.",
	"manifest_version": 2,
	"homepage_url": "http://www.thetomharrison.com/flickr-gradient-block",
	"content_scripts": [
		{
			"all_frames": true,
			"css": ["nogradients.css"],
			"js": ["nogradients.js"],
			"matches": ["*://*.flickr.com/"]
		}
	],
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	}
}

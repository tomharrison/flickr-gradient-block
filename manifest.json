{
	"name": "Flickr Gradient Block",
	"version": "0.0.1",
	"description": "Removes gradient overlays from images in your Flickr contacts feed.",
	"manifest_version": 2,
	"homepage_url": "http://www.thetomharrison.com/flickr-gradient-block",
	"content_scripts": [
		{
			"all_frames": true,
			"css": ["nogradients.css"],
			"js": ["nogradients.js"],
			"matches": ["*://*.flickr.com/"]
		}
	]
}
